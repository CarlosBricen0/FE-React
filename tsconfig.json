{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "es6",
    "module": "commonjs",
    "types": [
      "node",
      "jest"
    ],
    "moduleResolution": "node",
    "sourceMap": true,
    "resolveJsonModule": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}